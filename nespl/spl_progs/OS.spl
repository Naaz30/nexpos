[pte + 11] = 84;    //Update user area page
[pte + 13] = 8*512; //User Stack Pointer update
[pte + 12] = 0;     // KPTR to 0
[pte + 14] = PTBR;
[pte + 15] = PTLR;


i = 0;
while(i < 10) do
	[PTBR + 2*i] = -1;
	[PTBR + 2*i + 1] = "0000";
	i = i + 1;
endwhile;

//Code
[PTBR+8] = 69;
[PTBR+9] = "0100";

//Stack
[PTBR+16] = 83;
[PTBR+17] = "0110";


start;




