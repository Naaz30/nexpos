//--------------------------------- IDLE PROGRAM --------------------------


SP = 76 * 512 + 1;
call BOOT_MODULE;



PTBR = PAGE_TABLE_BASE;
PTLR = 10;

[PROCESS_TABLE + 0] = 0; 
[PROCESS_TABLE + 1] = 0;
[PROCESS_TABLE + 4] = RUNNING;
[PROCESS_TABLE + 11] = 76;   
[PROCESS_TABLE + 12] = 0;  
[PROCESS_TABLE + 13] = 8*512; 
[PROCESS_TABLE + 14] = PTBR;
[PROCESS_TABLE + 15] = PTLR;




alias i R1;

i = 0;
while(i < 10) do
	[PTBR + 2*i] = -1;
	[PTBR + 2*i + 1] = "0000";
	i = i + 1;
endwhile;


//Code
[PTBR+8] = 69;
[PTBR+9] = "0100";


//Stack
[PTBR+16] = 70;
[PTBR+17] = "0110";



[70*512] = [69*512 + 1];
[SYSTEM_STATUS_TABLE + 1] = 0;


SP = 8*512;

//--------------------------------- IDLE 2 PROGRAM --------------------------


PTBR = PAGE_TABLE_BASE + 14*20;
PTLR = 10;

alias pte R2;
pte = PROCESS_TABLE + 14*16;

[pte + 0] = 0; //tick to 0
[pte + 1] = 14;
